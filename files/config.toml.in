[build]
extended = true
# This is in addition to host's triple: x86_64-unknown-linux-gnu in our case
target = ["i686-unknown-linux-gnu"]
tools = ["cargo"]
vendor = true

[install]
libdir = "LIBDIR"
prefix = "PREFIX"

[llvm]
ninja = true

[rust]
channel = "stable"
rpath = false

[target.i686-unknown-linux-gnu]
ar = "/usr/bin/ar"

[target.x86_64-unknown-linux-gnu]
ar = "/usr/bin/ar"
