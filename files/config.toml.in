[build]
cargo = "PREFIX/bin/cargo"
docs = false
rustc = "PREFIX/bin/rustc"
# This is in addition to host's triple: x86_64-unknown-linux-gnu in our case
target = ["i686-unknown-linux-gnu"]
vendor = true

[install]
libdir = "LIBDIR"
prefix = "PREFIX"

[llvm]
ninja = true
targets = "X86"

[rust]
channel = "stable"
rpath = false

[target.i686-unknown-linux-gnu]
ar = "/usr/bin/ar"

[target.x86_64-unknown-linux-gnu]
ar = "/usr/bin/ar"
